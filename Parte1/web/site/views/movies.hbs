<h1>{{title}}</h1>
<ul style="list-style: none;">
    {{#each this.movies}}
      <li>
          <div>
              <p style=""> {{this.title}} </p>
              <a href="/movies/{{this.id}}"> <img src = {{this.image}} style="width:128px;height:176px;" ></a>
          </div>
      </li>
    {{/each}}
</ul>

